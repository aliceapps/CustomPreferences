<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomListPreference">
        <attr name="list_item_layout" format="integer" />
        <attr name="list_dialog_animation" format="integer" />
    </declare-styleable>

    <declare-styleable name="CustomTimePreference">
        <attr name="picker_dialog_theme" format="integer" />
        <attr name="picker_dialog_animation" format="integer" />
        <attr name="picker_dialog_24_format" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomSeekbar">
        <attr name="android:min"/>
        <attr name="android:max"/>
        <attr name="seekbar_type_text" format="integer"/>
    </declare-styleable>
</resources>